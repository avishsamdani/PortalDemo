<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <webLinks>
        <fullName>EnrollmentOpportunityLogACall</fullName>
        <availability>online</availability>
        <description>Log a Call from the EO screen, with the EO&apos;s contact already populated.</description>
        <displayType>massActionButton</displayType>
        <encodingKey>UTF-8</encodingKey>
        <linkType>url</linkType>
        <masterLabel>Log a Call</masterLabel>
        <openType>replace</openType>
        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>/00T/e?title=Call&amp;what_id={!Enrollment_Opportunity__c.Id}&amp;followup=1&amp;tsk5=Call&amp;retURL=%2F{!Enrollment_Opportunity__c.Id}&amp;tsk2={!Enrollment_Opportunity__c.Applicant_First_Name__c}+{!Enrollment_Opportunity__c.Applicant_Last_Name__c}</url>
    </webLinks>
    <webLinks>
        <fullName>EnrollmentOpportunityNewTask</fullName>
        <availability>online</availability>
        <description>Opens the &quot;New Task&quot; window from an EO, with the Contact information populated.</description>
        <displayType>massActionButton</displayType>
        <encodingKey>UTF-8</encodingKey>
        <linkType>url</linkType>
        <masterLabel>New Task</masterLabel>
        <openType>replace</openType>
        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>/00T/e?what_id={!Enrollment_Opportunity__c.Id}&amp;retURL=%2F{!Enrollment_Opportunity__c.Id}&amp;tsk2={!Enrollment_Opportunity__c.Applicant_First_Name__c}+{!Enrollment_Opportunity__c.Applicant_Last_Name__c}</url>
    </webLinks>
    <webLinks>
        <fullName>EnrollmentOpportunitySendAnEmail</fullName>
        <availability>online</availability>
        <description>Goes to the Send an Email screen, and populates the Contact field with the Applicant of the Enrollment Opportunity.</description>
        <displayType>massActionButton</displayType>
        <encodingKey>UTF-8</encodingKey>
        <linkType>url</linkType>
        <masterLabel>Send an Email</masterLabel>
        <openType>replace</openType>
        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>/_ui/core/email/author/EmailAuthor?p3_lkid={!Enrollment_Opportunity__c.Id}&amp;p2_lkid={!Enrollment_Opportunity__c.ApplicantId__c}&amp;retURL=%2F{!Enrollment_Opportunity__c.Id}</url>
    </webLinks>
</CustomObject>
